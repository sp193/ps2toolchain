+	.set	noreorder
+	.set	nomacro
+	nop
+	bne	$6,$4,$L19
+	addu	$3,$3,1
+	.set	macro
+	.set	reorder
+
+$L7:
+	.set	noreorder
+	.set	nomacro
+	j	$31
+	move	$2,$8
+	.set	macro
+	.set	reorder
+
+	.end	memmove
