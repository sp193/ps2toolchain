+	sq	$30, O_FP($4)
+	sq	$29, O_SP($4)
+	sq	$31, O_RA($4)
+
+	swc1    $f20, O_F20($4)
+	swc1    $f21, O_F21($4)
+	swc1    $f22, O_F22($4)
+	swc1    $f23, O_F23($4)
+	swc1    $f24, O_F24($4)
+	swc1    $f25, O_F25($4)
+	swc1    $f26, O_F26($4)
+	swc1    $f27, O_F27($4)
+	swc1    $f28, O_F28($4)
+	swc1    $f29, O_F29($4)
+	swc1    $f30, O_F30($4)
+	swc1    $f31, O_F31($4)
+
