+   to this position using strcpy to complete concatenation. */
+
+$L9:
+$L15:
+	lb	$2,0($4)
+	.set	noreorder
+	.set	nomacro
+	nop
+	nop
+	nop
+	nop
+	bnel	$2,$0,$L9
+	addu	$4,$4,1
+	.set	macro
+	.set	reorder
+
+	jal	strcpy
+	move	$2,$16
+	lq	$31,16($sp)
+	lq	$16,0($sp)
+	.set	noreorder
+	.set	nomacro
+	j	$31
+	addu	$sp,$sp,32
+	.set	macro
+	.set	reorder
+
+	.end	strcat
