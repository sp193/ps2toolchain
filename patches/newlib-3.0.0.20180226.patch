+	lq	$18, O_S2($4)
+	lq	$19, O_S3($4)
+	lq	$20, O_S4($4)
+	lq	$21, O_S5($4)
+	lq	$22, O_S6($4)
+	lq	$23, O_S7($4)
+	lq	$30, O_FP($4)
+	lq	$29, O_SP($4)
+	lq	$31, O_RA($4)
