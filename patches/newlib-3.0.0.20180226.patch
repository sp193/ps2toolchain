+#ifndef __OPTIMIZE_SIZE__
+
+	andi	$2,$4,0x7
+	.set	noreorder
+	.set	nomacro
+	bne	$2,$0,$L24
+	andi	$5,$5,0x00ff
+	.set	macro
+	.set	reorder
