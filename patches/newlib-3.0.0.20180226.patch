+	.set	noreorder
+	.set	nomacro
+	bne	$2,$0,$L15
+	sq	$31,16($sp)
+	.set	macro
+	.set	reorder
+
+	andi	$2,$16,0xf
+	dli	$3,0x0101010101010101
