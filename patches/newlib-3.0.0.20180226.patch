+	dsubu	$3,$6,$3
+	nor	$2,$0,$6
+	and	$3,$3,$2
+	and	$3,$3,$4
+	.set	noreorder
+	.set	nomacro
+	bne	$3,$0,$L9
+	move	$4,$16
+	.set	macro
+	.set	reorder
+
+	dli	$7,0x0101010101010101
+	addu	$6,$16,8
+$L16:
+	ld	$2,0($6)
+	#nop
+	nor	$3,$0,$2
+	dsubu	$2,$2,$7
+	and	$2,$2,$3
+	and	$2,$2,$4
+	.set	noreorder
+	.set	nomacro
+	beql	$2,$0,$L16
+	addu	$6,$6,8
+	.set	macro
+	.set	reorder
+
+$L4:
+	move	$4,$6
+
+#else /* __OPTIMIZE_SIZE__ */
+
+	sq	$31,16($sp)
+
+#endif /* __OPTIMIZE_SIZE__ */
