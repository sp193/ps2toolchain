+	.set	noreorder
+	.set	nomacro
+	bne	$2,$0,$L3
+	ld	$6,0($16)
+	.set	macro
+	.set	reorder
+
+/* dest string is quadword aligned, load a quadword at a time looking for a null terminator.
+   Loop until null terminator is found and then search last quadword for actual position
+   of null terminator. */
+
+	lq	$2,0($16)
+	pcpyld	$7,$3,$3
+	pcpyld	$8,$4,$4
+	psubb	$3,$2,$7
+	pnor	$2,$0,$2
+	pand	$3,$3,$2
+	pand	$3,$3,$8
