+#define O_F30   0xd8
+#define O_F31   0xdc
+
+/* int setjmp (jmp_buf);  */
+	.globl	setjmp
+	.ent	setjmp
+setjmp:
+	.frame	$sp,0,$31
+
+	sq	$16, O_S0($4)
+	sq	$17, O_S1($4)
+	sq	$18, O_S2($4)
+	sq	$19, O_S3($4)
