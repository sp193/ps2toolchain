+	nop
+	beql	$2,$0,$L16
+	sd	$3,0($7)
+	.set	macro
+	.set	reorder
+
+	move	$3,$7
+
+#else  /* __OPTIMIZE_SIZE__ */
+
+	move	$3,$4
+
+#endif /* __OPTIMIZE_SIZE__ */
